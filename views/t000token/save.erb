    <label>C000 Fecha Creacion<br><input type="text" name="t000Token[c000FechaCreacion]" value="<%= t000Token["c000FechaCreacion"] %>" /></label>
  </p>
  <p>
    <label>C000 Fecha Actualizaci n<br><input type="text" name="t000Token[c000FechaActualización]" value="<%= t000Token["c000FechaActualización"] %>" /></label>
  </p>
  <p>
    <input type="submit" value="Save" />
  </p>
</form>

<%== linkTo("Show", urla("show", t000Token["c000Id"])) %> |
<%== linkTo("Back", urla("index")) %>
</body>
</html>
